<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ShopPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.Entity.Item</class>
    <class>com.Entity.Grn</class>
    <class>com.Entity.Login</class>
    <class>com.Entity.Category</class>
    <class>com.Entity.Customertype</class>
    <class>com.Entity.InvoiceHasItem</class>
    <class>com.Entity.Loginhistory</class>
    <class>com.Entity.UserHasAction</class>
    <class>com.Entity.ItemHasImage</class>
    <class>com.Entity.Service</class>
    <class>com.Entity.User</class>
    <class>com.Entity.Stock</class>
    <class>com.Entity.Supplier</class>
    <class>com.Entity.GrnHasItem</class>
    <class>com.Entity.Brand</class>
    <class>com.Entity.Usertype</class>
    <class>com.Entity.PurchaseOrderHasItem</class>
    <class>com.Entity.Tax</class>
    <class>com.Entity.Batch</class>
    <class>com.Entity.Purchaseorder</class>
    <class>com.Entity.Discount</class>
    <class>com.Entity.Subcategory</class>
    <class>com.Entity.Customer</class>
    <class>com.Entity.Serviceinvoice</class>
    <class>com.Entity.Servicetype</class>
    <class>com.Entity.Invoice</class>
    <class>com.Entity.Agent</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/msdb?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.password" value="1234"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
